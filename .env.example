# Example environment configuration
# Copy this to .env and fill in your actual values

# Zyte Scrapy Cloud Configuration
SCRAPINGHUB_API_KEY=your_api_key_here
SCRAPINGHUB_PROJECT_ID=your_project_id_here

# Optional: Custom settings
DOWNLOAD_DELAY=2
CONCURRENT_REQUESTS=8
USER_AGENT=coupon_scraper (+http://yourdomain.com)

# Database settings (if you want to store data in a database)
DATABASE_URL=sqlite:///coupons.db

# API keys for coupon sites (if required)
RETAILMENOT_API_KEY=your_key_here
COUPONS_COM_API_KEY=your_key_here
